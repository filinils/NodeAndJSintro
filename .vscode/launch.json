    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "NPM Runtime",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/NPMRuntime/index.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Dig",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/theDigger.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "es modules",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/BasicJS/esimporter.mjs"
        }
    ]
}